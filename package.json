    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/parser": "^8.7.0",
    "@vitejs/plugin-react": "^4.3.1",
    "allure-commandline": "^2.36.0",
    "allure-playwright": "^3.4.3",
    "babel-jest": "^30.0.2",
    "concurrently": "^9.0.1",
    "cross-env": "^7.0.3",
    "cypress-vite": "^1.5.0",
    "devextreme-themebuilder": "24.1.4",
    "eslint": "^9.11.1",
    "eslint-plugin-react": "^7.37.5",
