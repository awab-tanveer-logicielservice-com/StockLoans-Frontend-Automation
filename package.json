{
  "name": "opshub-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "run-and-test": "concurrently --kill-others --names dev,cypress -c yello,green  -P \"yarn dev\" \"yarn cypress:run -- {@}\" --",
    "build-and-test": "concurrently --kill-others --names dev,preview,cypress -c yellow,green,blue -P  \"yarn build\" \"yarn preview\" \"yarn cypress:run -- {@}\" --",
    "preview-and-test": "concurrently --kill-others --names dev,cypress -c yello,green -P \"yarn preview\" \"yarn cypress:run -- {@}\" --",
    "cmd-to-test": "yarn run run-and-test -- --spec cypress/e2e/test.cy.ts",
    "test": "jest",
    "jest": "jest --runInBand",
    "test:e2e": "playwright test"
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@fontsource/inter": "^5.1.0",
    "@mui/icons-material": "^6.1.1",
    "@mui/material": "^6.1.1",
    "@mui/x-data-grid": "^7.20.0",
    "@preact/signals-react": "^2.2.0",
    "@types/jest": "^29.5.14",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "axios": "^1.7.7",
    "babel-eslint": "^10.1.0",
    "devextreme": "24.1.4",
    "devextreme-aspnet-data-nojquery": "^5.0.0-beta",
    "devextreme-react": "24.1.4",
    "eslint-plugin-import": "^2.30.0",
    "jest-environment-jsdom": "^30.0.0-beta.3",
    "moment": "^2.30.1",
    "react": "^19.1.0",
    "react-cookie": "^7.2.0",
    "react-dom": "^19.1.0",
    "react-otp-input": "^3.1.1",
    "react-router-dom": "^6.26.2",
    "react-toastify": "^10.0.6",
    "ts-jest": "^29.4.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.27.1",
